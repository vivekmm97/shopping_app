<h1 class="title">Users Home Page</h1>

<div class="box">
<div class="block1">
    <h1 class="reg">Registration</h1>
    <div class="links">

    <%= link_to "Create Your Account", users_new_path %>

    </div>
</div>
<div class="block2">
    <h1 class="log">Login</h1>
</div>

</div>
<table>
<tr>
<td class="header">Name</td>
<td class="header">Email</td>
<td class="header">Place</td>
</tr>
<% @users.each do |user|%>
<tr>
<td><%= user.name%></td>
<td><%= user.email%></td>
<td><%= user.place%></td>
</tr>

<%end%>